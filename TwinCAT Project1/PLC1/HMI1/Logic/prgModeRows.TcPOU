				iReturnPrompt		:=,
				iConfig				:=RowConfig.OnlyRightNoVal,
				iAdvanceNumber		:=0,
				iReturnNumber		:=0,
				iUnits				:='',
				iPosn				:='',
				NumRows				:=tNumRows,
				MotionRowLocation	:=gModeAlternate.gModeRows.gMotionRow8,
				MotionRowBtnLocation:=gModeAlternate.gModeButtons.gMotionRow8btn
				);
				
//manually set row number for textlist offset				
