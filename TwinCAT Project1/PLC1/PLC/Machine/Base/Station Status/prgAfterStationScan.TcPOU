	S2_Global.hmiHeader.AnyStationWarningColour		:= HMIColour.colourYellow;
	S3_Global.hmiHeader.AnyStationWarningColour		:= HMIColour.colourYellow;
	S4_Global.hmiHeader.AnyStationWarningColour		:= HMIColour.colourYellow;
	S5_Global.hmiHeader.AnyStationWarningColour		:= HMIColour.colourYellow;
	S6_Global.hmiHeader.AnyStationWarningColour		:= HMIColour.colourYellow;
	
