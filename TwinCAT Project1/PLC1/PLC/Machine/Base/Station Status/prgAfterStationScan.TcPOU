	S6_Global.hmiHeader.isAnyWarningState				:= FALSE;	
END_IF

	//set indicators to 0
	S1_Global.station.hmiHeader.AnyStationWarningHeader := 0;
	S2_Global.hmiHeader.AnyStationWarningHeader := 0;
	S3_Global.hmiHeader.AnyStationWarningHeader := 0;
	S4_Global.hmiHeader.AnyStationWarningHeader := 0;
	S5_Global.hmiHeader.AnyStationWarningHeader := 0;
	S6_Global.hmiHeader.AnyStationWarningHeader := 0;

//write station 1 is in Warning
IF 	S1_Global.station.hmiHeader.isWarningState THEN
