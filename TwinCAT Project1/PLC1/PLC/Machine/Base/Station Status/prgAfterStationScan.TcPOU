	S1_Global.station.hmiHeader.AnyStationWarningHeader := 5;
	S2_Global.hmiHeader.AnyStationWarningHeader := 5;
	S3_Global.hmiHeader.AnyStationWarningHeader := 5;
	S4_Global.hmiHeader.AnyStationWarningHeader := 5;
	S5_Global.hmiHeader.AnyStationWarningHeader := 5;
