	S2_Global.hmiHeader.AnyStationWarningHeader := 6;
	S3_Global.hmiHeader.AnyStationWarningHeader := 6;
	S4_Global.hmiHeader.AnyStationWarningHeader := 6;
	S5_Global.hmiHeader.AnyStationWarningHeader := 6;
