	S3_Global.hmiHeader.AnyStationWarningHeader := 99;
	S4_Global.hmiHeader.AnyStationWarningHeader := 99;
	S5_Global.hmiHeader.AnyStationWarningHeader := 99;
	S6_Global.hmiHeader.AnyStationWarningHeader := 99;
	
	S1_Global.station.hmiHeader.isWarningState := TRUE;
	S2_Global.hmiHeader.isWarningState := TRUE;
	S3_Global.hmiHeader.isWarningState := TRUE;
	S4_Global.hmiHeader.isWarningState := TRUE;
	S5_Global.hmiHeader.isWarningState := TRUE;
	S6_Global.hmiHeader.isWarningState := TRUE;
	
	S1_Global.station.hmiHeader.WarningStateHeader := 1;
	S2_Global.hmiHeader.WarningStateHeader := 1;
	S3_Global.hmiHeader.WarningStateHeader := 1;
	S4_Global.hmiHeader.WarningStateHeader := 1;
	S5_Global.hmiHeader.WarningStateHeader := 1;
	S6_Global.hmiHeader.WarningStateHeader := 1;
END_IF]]></ST>
