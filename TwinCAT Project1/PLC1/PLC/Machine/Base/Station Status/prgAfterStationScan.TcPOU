	S3_Global.hmiHeader.AnyStationWarningHeaderActive := System.OneSecond;
	S4_Global.hmiHeader.AnyStationWarningHeaderActive := System.OneSecond;
	S5_Global.hmiHeader.AnyStationWarningHeaderActive := System.OneSecond;
