	S1_Global.station.hmiHeader.AnyStationWarningHeader := 4;
	S2_Global.hmiHeader.AnyStationWarningHeader := 4;
	S3_Global.hmiHeader.AnyStationWarningHeader := 4;
	S4_Global.hmiHeader.AnyStationWarningHeader := 4;
	S5_Global.hmiHeader.AnyStationWarningHeader := 4;
